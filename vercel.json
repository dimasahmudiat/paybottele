{
  "version": 2,
  "functions": {
    "api/bot.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/cron.js": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/bot",
      "dest": "/api/bot.js",
      "methods": ["POST"]
    },
    {
      "src": "/cron",
      "dest": "/api/cron.js",
      "methods": ["GET", "POST"]
    }
  ],
 
