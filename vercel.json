{
  "version": 2,
  "functions": {
    "api/bot.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/cron.js": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "src": "/bot",
      "dest": "/api/bot.js",
      "methods": ["POST"]
    },
    {
      "src": "/cron",
      "dest": "/api/cron.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "DB_HOST": "",
    "DB_USER": "",
    "DB_PASSWORD": "",
    "DB_NAME": "",
    "BOT_TOKEN": "",
    "ADMIN_CHAT_ID": "",
    "API_KEY": "",
    "MERCHANT_CODE": "",
    "WELCOME_IMAGE": "https://dimzmods.my.id/demobot/img/contoh1.jpg",
    "NODE_ENV": "production"
  }
}
